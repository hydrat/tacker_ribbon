<mso:customUI xmlns:x1="http://schemas.microsoft.com/office/2009/07/customui/macro" xmlns:mso="http://schemas.microsoft.com/office/2009/07/customui"  xmlns:tacker="http://hydrat.ch/2011/08/tacker" loadImage="OnLoadImage">
  <mso:ribbon startFromScratch="true">
    <mso:contextualTabs>
      <mso:tabSet idMso="TabSetFormReportExtensibility">
        <mso:tab id="standort" label="Standort">
          <mso:group id="standort_general" label="Allgemein" imageMso="AppointmentColor2" autoScale="true">
            <mso:control idQ="mso:FilePrintQuick" size="large" visible="true"/>
            <mso:control idQ="mso:Undo" size="large" visible="true"/>
            <mso:control idQ="mso:Redo" size="large" visible="true"/>
            <mso:control idQ="mso:RecordsDeleteRecord" size="large" visible="true"/>
          </mso:group>
          <mso:group id="standort_forms" label="Standorte" autoScale="true">
            <mso:button id="standort_suchen" label="Suchen" size="large" image="search_big.bmp" onAction="standort.standort_suchen" visible="true"/>
            <mso:button id="neuer_standort" label="Neu" image="add_big.bmp" onAction="standort.neuer_standort" visible="true"/>
          </mso:group>
          <mso:group id="fixstelle_reports" label="Fixstelle" autoScale="true">
            <mso:button id="fix_packages" label="Fixstelle mit Packages" image="report_small.bmp" onAction="standort.einzelfixstelle_drucken" visible="true"/>
            <mso:button id="fix_details" label="Fixstelle Details" image="report_small.bmp" onAction="standort.einzelfixstelle_details" visible="true"/>
            <mso:button id="fix_platzliste" label="Platzliste" image="report_small.bmp" onAction="standort.plaetze_drucken" visible="true"/>
          </mso:group>
          <mso:group id="standort_reports" label="Berichte" autoScale="true">
            <mso:button id="standort_vertrag" label="Deckblatt Vertrag" image="report_small.bmp" onAction="standort.deckblatt_drucken" visible="true"/>
          </mso:group>
          <mso:group id="standort_close" label="Allgemein" imageMso="AppointmentColor2" autoScale="true">
            <mso:control idQ="mso:MasterViewClose" size="large" visible="true"/>
          </mso:group>
        </mso:tab>
      </mso:tabSet>
    </mso:contextualTabs>
  </mso:ribbon>
</mso:customUI>
