<mso:customUI xmlns:x1="http://schemas.microsoft.com/office/2009/07/customui/macro" xmlns:mso="http://schemas.microsoft.com/office/2009/07/customui"  xmlns:tacker="http://hydrat.ch/2011/08/tacker" loadImage="OnLoadImage">
  <mso:ribbon startFromScratch="true">
    <mso:contextualTabs>
      <mso:tabSet idMso="TabSetFormReportExtensibility">
        <mso:tab id="ortschaft" label="Ortschaft">
          <mso:group id="ortschaft_general" label="Allgemein" imageMso="AppointmentColor2" autoScale="true">
            <mso:control idQ="mso:FilePrintQuick" size="large" visible="true"/>
            <mso:control idQ="mso:Undo" size="large" visible="true"/>
            <mso:control idQ="mso:Redo" size="large" visible="true"/>
            <mso:control idQ="mso:RecordsDeleteRecord" size="large" visible="true"/>
          </mso:group>
          <mso:group id="ortschaft_forms" label="Ortschaften" autoScale="true">
            <mso:button id="ortschaft_suchen" label="Suchen" size="large" image="search_big.bmp" onAction="ortschaft.ortschaft_suchen" visible="true"/>
            <mso:button id="neue_ortschaft" label="Neu" image="add_big.bmp" onAction="ortschaft.neue_ortschaft" visible="true"/>
          </mso:group>
          <mso:group id="ortschaft_reports" label="Berichte" autoScale="true">
            <mso:button id="ortschaft_liste" label="Ortschaftsliste" image="report_small.bmp" onAction="ortschaft.ortschaftliste_drucken" visible="true"/>
            <mso:button id="standort_liste" label="Standortliste" image="report_small.bmp" onAction="ortschaft.standortliste_drucken" visible="true"/>
          </mso:group>
          <mso:group id="ortschaft_close" label="Allgemein" imageMso="AppointmentColor2" autoScale="true">
            <mso:control idQ="mso:MasterViewClose" size="large" visible="true"/>
          </mso:group>
        </mso:tab>
      </mso:tabSet>
    </mso:contextualTabs>
  </mso:ribbon>
</mso:customUI>
