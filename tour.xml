<mso:customUI xmlns:x1="http://schemas.microsoft.com/office/2009/07/customui/macro" xmlns:mso="http://schemas.microsoft.com/office/2009/07/customui"  xmlns:tacker="http://hydrat.ch/2011/08/tacker" loadImage="OnLoadImage">
  <mso:ribbon startFromScratch="true">
    <mso:contextualTabs>
      <mso:tabSet idMso="TabSetFormReportExtensibility">
        <mso:tab id="tour" label="Tour">
          <mso:group id="tour_general" label="Allgemein" imageMso="AppointmentColor2" autoScale="true">
            <mso:control idQ="mso:FilePrintQuick" size="large" visible="true"/>
            <mso:control idQ="mso:Undo" size="large" visible="true"/>
            <mso:control idQ="mso:Redo" size="large" visible="true"/>
            <mso:control idQ="mso:RecordsDeleteRecord" size="large" visible="true"/>
          </mso:group>
          <mso:group id="tour_forms" label="Touren" autoScale="true">
            <mso:button id="tour_suchen" label="Suchen" size="large" image="search_big.bmp" onAction="tour.tour_suchen" visible="true"/>
            <mso:button id="neue_tour" label="Neu" image="add_big.bmp" onAction="tour.neue_tour" visible="true"/>
          </mso:group>
          <mso:group id="tour_reports" label="Berichte" autoScale="true">
            <mso:button id="tour_liste" label="Tour Liste" image="report_small.bmp" onAction="tour.tourliste_drucken" visible="true"/>
          </mso:group>
          <mso:group id="tour_close" label="Allgemein" imageMso="AppointmentColor2" autoScale="true">
            <mso:control idQ="mso:MasterViewClose" size="large" visible="true"/>
          </mso:group>
        </mso:tab>
      </mso:tabSet>
    </mso:contextualTabs>
  </mso:ribbon>
</mso:customUI>
