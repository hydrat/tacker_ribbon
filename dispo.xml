<mso:customUI xmlns:x1="http://schemas.microsoft.com/office/2009/07/customui/macro" xmlns:mso="http://schemas.microsoft.com/office/2009/07/customui"  xmlns:tacker="http://hydrat.ch/2011/08/tacker" loadImage="OnLoadImage">
  <mso:ribbon startFromScratch="true">
    <mso:contextualTabs>
      <mso:tabSet idMso="TabSetFormReportExtensibility">
        <mso:tab id="dispo" label="Dispos">
          <mso:group id="dispo_general" label="Allgemein" imageMso="AppointmentColor2" autoScale="true">
            <mso:control idQ="mso:FilePrintQuick" size="large" visible="true"/>
            <mso:control idQ="mso:Undo" size="large" visible="true"/>
            <mso:control idQ="mso:Redo" size="large" visible="true"/>
            <mso:control idQ="mso:RecordsDeleteRecord" size="large" visible="true"/>
          </mso:group>
          <mso:group id="dispo_forms" label="Dispo" autoScale="true">
            <mso:button id="neue_dispo" label="Neu" size="large" image="add_big.bmp" onAction="dispo.neue_dispo" visible="true"/>
          </mso:group>
          <mso:group id="dispo_reports" label="Berichte" autoScale="true">
            <mso:button id="dispo_touren" label="Touren" image="report_small.bmp" onAction="dispo.dispo_touren" visible="true"/>
            <mso:button id="routen" label="Routen" image="report_small.bmp" onAction="dispo.dispo_routen" visible="true"/>
<!-- 
          </mso:group>
          <mso:group id="dispo_reports_hoch" label="Berichte Hoch" autoScale="true">
            <mso:button id="dispo_hoch" label="Hoch" image="report_small.bmp" onAction="dispo.dispo_hoch" visible="true"/>
            <mso:button id="dispo_hoch_notes" label="Hoch Bemerkungen" image="report_small.bmp" onAction="dispo.dispo_hoch_notes" visible="true"/>
            <mso:button id="dispo_hoch_blacklist" label="Hoch Blacklist" image="report_small.bmp" onAction="dispo.dispo_hoch_blacklist" visible="true"/>
          </mso:group>
          <mso:group id="dispo_reports_quer" label="Berichte Quer" autoScale="true">
            <mso:button id="dispo_quer_2" label="Quer" image="report_small.bmp" onAction="dispo.dispo_quer_2" visible="true"/>
            <mso:button id="dispo_quer_2_notes" label="Quer Bemerkungen" image="report_small.bmp" onAction="dispo.dispo_quer_2_notes" visible="true"/>
            <mso:button id="dispo_quer_2_blacklist" label="Quer Blacklist" image="report_small.bmp" onAction="dispo.dispo_quer_2_blacklist" visible="true"/>
            <mso:button id="dispo_quer_3" label="Quer3" image="report_small.bmp" onAction="dispo.dispo_quer_3" visible="true"/>
            <mso:button id="dispo_quer_4" label="Quer4" image="report_small.bmp" onAction="dispo.dispo_quer_4" visible="true"/>
          </mso:group>
          <mso:group id="dispo_reports_ander" label="Andere Berichte" autoScale="true">
            <mso:button id="dispo_charts" label="Charts" image="report_small.bmp" onAction="dispo.dispo_chart" visible="true"/>
            <mso:button id="dispo_fixnetz" label="Routen" image="report_small.bmp" onAction="dispo.dispo_fixnetz" visible="true"/>
            <mso:button id="dispo_packages" label="Fix/Flyer" image="report_small.bmp" onAction="dispo.dispo_packages" visible="true"/>
  -->
            <mso:button id="dispo_homebase" label="Homebase" image="report_small.bmp" onAction="dispo.homebase" visible="true"/>
          </mso:group>
          <mso:group id="dispo_close" label="Allgemein" imageMso="AppointmentColor2" autoScale="true">
            <mso:control idQ="mso:MasterViewClose" size="large" visible="true"/>
          </mso:group>
        </mso:tab>
      </mso:tabSet>
    </mso:contextualTabs>
  </mso:ribbon>
</mso:customUI>
