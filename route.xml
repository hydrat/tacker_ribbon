<mso:customUI xmlns:x1="http://schemas.microsoft.com/office/2009/07/customui/macro" xmlns:mso="http://schemas.microsoft.com/office/2009/07/customui"  xmlns:tacker="http://hydrat.ch/2011/08/tacker" loadImage="OnLoadImage">
  <mso:ribbon startFromScratch="true">
    <mso:contextualTabs>
      <mso:tabSet idMso="TabSetFormReportExtensibility">
        <mso:tab id="route" label="Route">
          <mso:group id="route_general" label="Allgemein" imageMso="AppointmentColor2" autoScale="true">
            <mso:control idQ="mso:FilePrintQuick" size="large" visible="true"/>
             <mso:control idQ="mso:Undo" size="large" visible="true"/>
            <mso:control idQ="mso:Redo" size="large" visible="true"/>
            <mso:control idQ="mso:RecordsDeleteRecord" size="large" visible="true"/>
         </mso:group>
          <mso:group id="route_forms" label="Routen" autoScale="true">
            <mso:button id="route_suchen" label="Suchen" size="large" image="search_big.bmp" onAction="route.route_suchen" visible="true"/>
            <mso:button id="neue_route" label="Neu" image="add_big.bmp" onAction="route.neue_route" visible="true"/>
          </mso:group>
          <mso:group id="route_reports" label="Berichte" autoScale="true">
            <mso:button id="route_liste" label="Route Liste" image="report_small.bmp" onAction="route.routeliste_drucken" visible="true"/>
          </mso:group>
          <mso:group id="route_close" label="Allgemein" imageMso="AppointmentColor2" autoScale="true">
            <mso:control idQ="mso:MasterViewClose" size="large" visible="true"/>
          </mso:group>
        </mso:tab>
      </mso:tabSet>
    </mso:contextualTabs>
  </mso:ribbon>
</mso:customUI>
