<mso:customUI xmlns:x1="http://schemas.microsoft.com/office/2009/07/customui/macro" xmlns:mso="http://schemas.microsoft.com/office/2009/07/customui"  xmlns:tacker="http://hydrat.ch/2011/08/tacker" loadImage="OnLoadImage">
  <mso:ribbon startFromScratch="true">
    <mso:contextualTabs>
      <mso:tabSet idMso="TabSetFormReportExtensibility">
        <mso:tab id="package" label="Packages">
          <mso:group id="package_general" label="Allgemein" imageMso="AppointmentColor2" autoScale="true">
            <mso:control idQ="mso:FilePrintQuick" size="large" visible="true"/>
            <mso:control idQ="mso:Undo" size="large" visible="true"/>
            <mso:control idQ="mso:Redo" size="large" visible="true"/>
            <mso:control idQ="mso:RecordsDeleteRecord" size="large" visible="true"/>
          </mso:group>
          <mso:group id="package_forms" label="Packages" autoScale="true">
            <mso:button id="package_suchen" label="Suchen" size="large" image="search_big.bmp" onAction="package.package_suchen" visible="true"/>
            <mso:button id="neues_package" label="Neu" image="add_big.bmp" onAction="package.neues_package" visible="true"/>
          </mso:group>
          <mso:group id="package_fix_reports" label="Fixnetz" autoScale="true">
            <mso:button id="einzelpackage_drucken" label="Einzelpackage" image="report_small.bmp" onAction="package.package_drucken" visible="true"/>
            <mso:button id="platzzuordnung_a2" label="Platzzuordnung A2" image="report_small.bmp" onAction="package.platzzuordnung_a2" visible="true"/>
            <mso:button id="platzzuordnung_a3" label="Platzzuordnung A3" image="report_small.bmp" onAction="packge.platzzuordnung_a3" visible="true"/>
          </mso:group>
          <mso:group id="package_fly_reports" label="Flyer" autoScale="true">
            <mso:button id="package_fly_drucken" label="Einzelpackage" image="report_small.bmp" onAction="package.package_flyer_drucken" visible="true"/>
          </mso:group>
          <mso:group id="package_close" label="Allgemein" imageMso="AppointmentColor2" autoScale="true">
            <mso:control idQ="mso:MasterViewClose" size="large" visible="true"/>
          </mso:group>
        </mso:tab>
      </mso:tabSet>
    </mso:contextualTabs>
  </mso:ribbon>
</mso:customUI>
