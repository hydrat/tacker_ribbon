<mso:customUI xmlns:x1="http://schemas.microsoft.com/office/2009/07/customui/macro" xmlns:mso="http://schemas.microsoft.com/office/2009/07/customui"  xmlns:tacker="http://hydrat.ch/2011/08/tacker" loadImage="OnLoadImage">
  <mso:ribbon startFromScratch="true">
    <mso:contextualTabs>
      <mso:tabSet idMso="TabSetFormReportExtensibility">
        <mso:tab id="kontakt" label="Kontakt">
          <mso:group id="kontakt_general" label="Allgemein" imageMso="AppointmentColor2" autoScale="true">
            <mso:control idQ="mso:FilePrintQuick" size="large" visible="true"/>
            <mso:control idQ="mso:Undo" size="large" visible="true"/>
            <mso:control idQ="mso:Redo" size="large" visible="true"/>
            <mso:control idQ="mso:RecordsDeleteRecord" size="large" visible="true"/>
          </mso:group>
          <mso:group id="kontakte_forms" label="Person" autoScale="true">
            <mso:button id="kop.kontakt_suchen" label="Suchen" image="search_small.bmp" onAction="kontakt.kontakt_suchen" visible="true"/>
            <mso:button id="kop.neuer_kop" label="Neu" image="add_small.bmp" onAction="kontakt.neue_kop" visible="true"/>
          </mso:group>
          <mso:group id="reports_ohne" label="Berichte ohne Logo" autoScale="true">
            <mso:button id="kop.drucken" label="Drucken" image="report_small.bmp" onAction="kontakt.kop_drucken" visible="true"/>
            <mso:button id="kop.beizettel" label="Beizettel" image="report_small.bmp" onAction="kontakt.kop_beizettel_brief" visible="true"/>
            <mso:button id="kop.couvert" label="Couvert" image="report_small.bmp" onAction="kontakt.kop_couvert_brief" visible="true"/>
          </mso:group>
          <mso:group id="reports_mit" label="Berichte mit Logo" autoScale="true">
            <mso:button id="kop.beizettel_mit" label="Beizettel" image="report_small.bmp" onAction="kontakt.kop_beizettel_logo" visible="true"/>
            <mso:button id="kop.couvert_mit" label="Couvert" image="report_small.bmp" onAction="kontakt.kop_couvert_logo" visible="true"/>
          </mso:group>
          <mso:group id="kop_close" label="Allgemein" imageMso="AppointmentColor2" autoScale="true">
            <mso:control idQ="mso:MasterViewClose" size="large" visible="true"/>
          </mso:group>
        </mso:tab>
      </mso:tabSet>
    </mso:contextualTabs>
  </mso:ribbon>
</mso:customUI>
