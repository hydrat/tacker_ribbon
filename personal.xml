<mso:customUI xmlns:x1="http://schemas.microsoft.com/office/2009/07/customui/macro" xmlns:mso="http://schemas.microsoft.com/office/2009/07/customui"  xmlns:tacker="http://hydrat.ch/2011/08/tacker" loadImage="OnLoadImage">
  <mso:ribbon startFromScratch="true">
    <mso:contextualTabs>
      <mso:tabSet idMso="TabSetFormReportExtensibility">
        <mso:tab id="personal" label="Personal">
          <mso:group id="personal_general" label="Allgemein" imageMso="AppointmentColor2" autoScale="true">
            <mso:control idQ="mso:FilePrintQuick" size="large" visible="true"/>
            <mso:control idQ="mso:Undo" size="large" visible="true"/>
            <mso:control idQ="mso:Redo" size="large" visible="true"/>
            <mso:control idQ="mso:RecordsDeleteRecord" size="large" visible="true"/>
          </mso:group>
          <mso:group id="personal_forms" label="Personal" autoScale="true">
            <mso:button id="per.personal_suchen" label="Suchen" image="search_small.bmp" onAction="personal.personal_suchen" visible="true"/>
            <mso:button id="per.neues_personal" label="Neu" image="add_small.bmp" onAction="personal.neues_personal" visible="true"/>
          </mso:group>
          <mso:group id="personal_reports" label="Berichte" autoScale="true">
            <mso:button id="per.drucken" label="Drucken" image="report_small.bmp" onAction="personal.kontakt_drucken" visible="true"/>
            <mso:button id="per.beizettel" label="Beizettel" image="report_small.bmp" onAction="personal.beizettel_drucken" visible="true"/>
            <mso:button id="per.couvert" label="Couvert" image="report_small.bmp" onAction="personal.couvert_drucken" visible="true"/>
          </mso:group>
          <mso:group id="personal_stunden" label="Lohnabrechnungen" autoScale="true">
            <mso:button id="per.stunden" label="Ãœbersicht" image="report_small.bmp" onAction="personal.stundenabrechnung_uebersicht" visible="true"/>
            <mso:button id="per.stunden_details" label="Details" image="report_small.bmp" onAction="personal.stundenabrechnung_details" visible="true"/>
          </mso:group>
          <mso:group id="personal_close" label="Allgemein" imageMso="AppointmentColor2" autoScale="true">
            <mso:control idQ="mso:MasterViewClose" size="large" visible="true"/>
          </mso:group>
        </mso:tab>
      </mso:tabSet>
    </mso:contextualTabs>
  </mso:ribbon>
</mso:customUI>
