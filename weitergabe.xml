<mso:customUI xmlns:x1="http://schemas.microsoft.com/office/2009/07/customui/macro" xmlns:mso="http://schemas.microsoft.com/office/2009/07/customui"  xmlns:tacker="http://hydrat.ch/2011/08/tacker" loadImage="OnLoadImage">
  <mso:ribbon startFromScratch="true">
    <mso:contextualTabs>
      <mso:tabSet idMso="TabSetFormReportExtensibility">
        <mso:tab id="weitergabe" label="Weitergaben">
          <mso:group id="weitergabe_general" label="Allgemein" imageMso="AppointmentColor2" autoScale="true">
            <mso:control idQ="mso:FilePrintQuick" size="large" visible="true"/>
            <mso:control idQ="mso:Undo" size="large" visible="true"/>
            <mso:control idQ="mso:Redo" size="large" visible="true"/>
            <mso:control idQ="mso:RecordsDeleteRecord" size="large" visible="true"/>
          </mso:group>
          <mso:group id="weitergabe_forms" label="Weitergabe" autoScale="true">
            <mso:button id="weitergabe_suchen" label="Suchen" size="large" image="search_big.bmp" onAction="weitergabe.weitergabe_suchen" visible="true"/>
            <mso:button id="neue_weitergabe" label="Neu" image="add_big.bmp" onAction="weitergabe.neue_weitergabe" visible="true"/>
          </mso:group>
          <mso:group id="weitergabe_reports" label="Lieferscheine" autoScale="true">
            <mso:button id="lieferscheine_ohne" label="Lieferscheine ohne" image="report_small.bmp" onAction="weitergaben.weitergabe_brief" visible="true"/>
            <mso:button id="lieferscheine_mit" label="Lieferscheine mit" image="report_small.bmp" onAction="weitergaben.weitergabe_logo" visible="true"/>
          </mso:group>
          <mso:group id="weitergabe_close" label="Allgemein" imageMso="AppointmentColor2" autoScale="true">
            <mso:control idQ="mso:MasterViewClose" size="large" visible="true"/>
          </mso:group>
        </mso:tab>
      </mso:tabSet>
    </mso:contextualTabs>
  </mso:ribbon>
</mso:customUI>
