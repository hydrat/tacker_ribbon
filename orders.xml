<mso:customUI xmlns:x1="http://schemas.microsoft.com/office/2009/07/customui/macro" xmlns:mso="http://schemas.microsoft.com/office/2009/07/customui"  xmlns:tacker="http://hydrat.ch/2011/08/tacker" loadImage="OnLoadImage">
  <mso:ribbon startFromScratch="true">
    <mso:contextualTabs>
      <mso:tabSet idMso="TabSetFormReportExtensibility">
        <mso:tab id="auftrag" label="Auftrag">
          <mso:group id="auftrag_general" label="Allgemein" imageMso="AppointmentColor2" autoScale="true">
            <mso:control idQ="mso:FilePrintQuick" size="large" visible="true"/>
            <mso:control idQ="mso:Undo" size="large" visible="true"/>
            <mso:control idQ="mso:Redo" size="large" visible="true"/>
            <mso:control idQ="mso:RecordsDeleteRecord" size="large" visible="true"/>
          </mso:group>
          <mso:group id="auftrag_forms" label="Auftrag" autoScale="true">
            <mso:button id="neuer_auftrag" label="Neu" size="large" image="add_small.bmp" onAction="order.new" visible="true"/>
          </mso:group>
          <mso:group id="berichte" label="Post" imageMso="AppointmentColor2" autoScale="true">
            <mso:button id="order.offerte" label="Offerte/Bestätigung" image="report_big.bmp" onAction="order.offerte" visible="true"/>
            <mso:button id="order.rechnung" label="Rechnung/Mahnung" image="report_big.bmp" onAction="order.rechnung" visible="true"/>
          </mso:group>
          <mso:group id="mails" label="Email" imageMso="AppointmentColor2" autoScale="true">
            <mso:button id="order.offerte_mail" label="Offerte/Bestätigung" image="report_big.bmp" onAction="order.offerte_mail" visible="true"/>
            <mso:button id="order.rechnung_mail" label="Rechnung/Mahnung" image="report_big.bmp" onAction="order.rechnung_mail" visible="true"/>
          </mso:group>
          <mso:group id="pdf" label="PDF" imageMso="AppointmentColor2" autoScale="true">
            <mso:button id="order.offerte_pdf" label="Offerte/Bestätigung" image="report_big.bmp" onAction="order.offerte_pdf" visible="true"/>
            <mso:button id="order.rechnung_pdf" label="Rechnung/Mahnung" image="report_big.bmp" onAction="order.rechnung_pdf" visible="true"/>
          </mso:group>
          <mso:group id="berichte_intern" label="Dispo/Partner" imageMso="AppointmentColor2" autoScale="true">
            <mso:button id="disposition_intern" label="Disposition" image="report_small.bmp" onAction="order.disposition_intern" visible="true"/>
            <mso:button id="lieferscheine_ohne" label="Lieferscheine Post" image="report_small.bmp" onAction="weitergaben.weitergabe_auftrag_brief" visible="true"/>
            <mso:button id="lieferscheine_mit" label="Lieferscheine Email" image="report_small.bmp" onAction="weitergaben.weitergabe_auftrag_logo" visible="true"/>
          </mso:group>
          <mso:group id="auftrag_close" label="Allgemein" imageMso="AppointmentColor2" autoScale="true">
            <mso:control idQ="mso:MasterViewClose" size="large" visible="true"/>
          </mso:group>
        </mso:tab>
      </mso:tabSet>
    </mso:contextualTabs>
  </mso:ribbon>
</mso:customUI>
